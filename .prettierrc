{
    "arrowParens": "avoid",
    "jsxSingleQuote": true,
    "printWidth": 250,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "plugins": ["@xeonlink/prettier-plugin-organize-attributes"],
    "attributeGroups": ["^class$", "^(id|name)$", "$DEFAULT", "^aria-"],
    "attributeSort": "ASC"
}
